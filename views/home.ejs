<%- include("partials/header"); -%>

<%  mensen.forEach(function(mensa){ %>

    <div class="card">
        <div class="card-header">
            <h3><%=mensa.name%></h3>
        </div>
        <div class="card-body">
          <h5 class="card-title"><%=mensa.address%></h5>
          <a href="/mensen/<%=mensa.id%>/<%=today%>/meals" class="btn btn-primary">Angebot Anzeigen</a>
        </div>
      </div>
      <% }) %>
      
<%- include("partials/footer"); -%>
